  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug backstage-demo",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "restart": true,
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["run-script", "start-backend", "--verbose"],
      "runtimeExecutable": "npm",
      "outFiles": [
        "${workspaceFolder}/dist-types/**/*.js"
      ],
      "env": {
        "LOG_LEVEL": "debug",
      }
    }
  ]
}